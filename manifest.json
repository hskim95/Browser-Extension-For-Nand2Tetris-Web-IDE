{
    "manifest_version": 3,
    "default_locale": "en",
    "name": "__MSG_name__",
    "version": "1.0.0",
    "description": "__MSG_description__",
    "icons": {},
    "action": {
        "default_icon": {
            "16": "Extension16.png",
            "32": "Extension32.png",
            "128": "Extension128.png"
        },
        "default_title": "__MSG_title__",
        "default_popup": "popup.html",
        "default_state": "disabled"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://nand2tetris.github.io/web-ide/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "scripting",
        "storage",
        "notifications"
    ],
    "host_permissions": [
        "https://nand2tetris.github.io/web-ide/*"
    ]
}
